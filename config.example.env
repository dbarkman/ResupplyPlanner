# It is strongly recommended to use a non-root user for database access.
# Ensure this user has the necessary privileges on the specified database.
RP_DB_HOST=127.0.0.1
RP_DB_PORT=5432
RP_DB_DATABASE=resupply_planner
RP_DB_USER=resupply_planner
RP_DB_PASSWORD=password

# EDDN Configuration
# This is the public endpoint for the EDDN data stream.
RP_EDDN_RELAY=tcp://eddn.edcd.io:9500
RP_EDDN_RELAY_TIMEOUT=600000

# Logging Configuration
# Valid levels: DEBUG, INFO, WARNING, ERROR, CRITICAL
RP_LOG_LEVEL=INFO
RP_LOG_RETENTION_DAYS=5